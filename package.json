{
    "name": "gemini-image-downloader",
    "version": "1.0.0",
    "description": "Electron app that downloads images from public Google Gemini chat links.",
    "main": "electron-main.js",
    "scripts": {
        "dev": "concurrently \"npm run osm:vite\" \"npm run osm:electron\"",
        "electron:dev": "npm run dev",
        "start": "npm run build && electron .",
        "osm:vite": "vite",
        "osm:electron": "wait-on tcp:5173 && electron . --dev",
        "build": "tsc && vite build",
        "dist:win": "npm run build && electron-builder --win",
        "dist:mac": "npm run build && electron-builder --mac",
        "postinstall": "electron-rebuild -f -w better-sqlite3",
        "rebuild": "electron-rebuild -f -w better-sqlite3"
    },
    "dependencies": {
        "@azure/msal-node": "^2.6.2",
        "@types/better-sqlite3": "^7.6.13",
        "axios": "^1.6.7",
        "better-sqlite3": "^12.6.2",
        "fs-extra": "^11.2.0",
        "lucide-react": "^0.344.0",
        "p-limit": "^7.2.0",
        "playwright": "^1.42.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "devDependencies": {
        "@tailwindcss/typography": "^0.5.19",
        "@types/react": "^18.2.64",
        "@types/react-dom": "^18.2.21",
        "@vitejs/plugin-react": "^4.2.1",
        "autoprefixer": "^10.4.18",
        "concurrently": "^8.2.2",
        "electron": "^29.1.1",
        "electron-builder": "^24.13.3",
        "electron-rebuild": "^3.2.9",
        "postcss": "^8.4.35",
        "tailwindcss": "^3.4.1",
        "typescript": "^5.4.2",
        "vite": "^5.1.5",
        "wait-on": "^7.2.0"
    }
}